<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="resource_access_rule" model="ir.rule">
        <field name="name">Resource Access Rule</field>
        <field name="model_id" ref="model_resource_resource"/>
        <field eval="True" name="global"/>
        <field name="domain_force">[('group_ids.group_id.id', 'in', [g.id for g in user.groups_id])]</field>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
    </record>
</odoo>
